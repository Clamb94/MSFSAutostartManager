<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>MSFSAutostartManager</PackageId>
    <Title>MSFSAutostartManager</Title>
    <Version>0.1.1</Version>
    <Authors>Axel Reinemuth</Authors>
    <Company>Axel Reinemuth</Company>
    <RepositoryUrl>https://github.com/Clamb94/MSFSAutostartManager</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.9.1" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="7.0.0" />
  </ItemGroup>

</Project>
